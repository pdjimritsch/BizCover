{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "application": {
        "name": "virtualcloudprovider",
        "AppConnectTimeout": 45
    },
    /* Authentication mode settings are : None, Basic, UseJwtBearer, UseIdentityWebApi, UseOpenIdConnect */
    "AuthenticationMode": "None",
    "ConnectionStrings": { /* change as per requirement */
        "Development": "",
        "Staging": "",
        "Production": ""
    },
    "ConnectionType": "", /* Valid types are: SqlServer, MySql, Memory, MongoDB. Defaults to Memory. */
    "Cookie": { /* change as per requirement */
        "Expiration": 30 /* 30 minutes */
    },
    "MongoDatabase": { /* change as per requirement */
        "Database": "local",
        "Enable": false,
        "Uri": "mongodb://localhost:27017"
    },
    "RequestTimeout": 30000, /* 30 seconds */
    "Security": { /* change as per requirement */
        "ApiKey": "DF215646-06E1-43AC-9758-362C1A3E9AE3", // replace with environment variables
        "KeyLength": 32,
        "VectorLength": 16
    },
    "SessionTimeout": 30, /* change as per requirement */
    "Subscribers": {
        "ApiSubscriber": "http://localhost:5250", /* correlates to the SecureAddress.Api launchsettings.json */
        "ApiTimeout": 30,
        "Trigger": "http://localhost:7071/HttpObserver",
        "TriggerTimeout": 30 /* 30 seconds */
    },
    "Swagger": {
        "Version": "v3"
    },
    "UseAzure": false,
    "UseCors": true,
    "UseSwagger": false,
    "WebApi": {
        "ApiRoute": "/api/",
        "RemoteUrl": ""
    }
}
